{
  "name": "license-to-plate",
  "description": "This is a boilerplate build with Vite, React 18, TypeScript, Vitest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "2.6.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "JackBox Society",
    "email": "J@j.com",
    "url": "https://github.com/brunston97/license-to-plate"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "VITE_APP_VERSION=$npm_package_version vite --open",
    "dev:server": "cd server && npm run dev",
    "prod:server": "cd server && npm run prod",
    "prod:client": "npm run build && firebase deploy --project license2plate-b4c6e --only hosting",
    "build": "VITE_APP_VERSION=$npm_package_version tsc && vite build",
    "buildAndDeploy": "npm run build && firebase deploy --project license2plate-b4c6e --only hosting",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "deploy": "npm run prod:server && npm run prod:client",
    "makePyEnv": "python3 -m venv .venv && source .venv/bin/activate && pip install -r parsePlates/requirements.txt"
  },
  "dependencies": {
    "@heroui/react": "2.8.7",
    "@heroui/use-theme": "^2.1.10",
    "axios": "^1.7.9",
    "framer-motion": "^11.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/axios": "^0.14.4",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.1",
    "daisyui": "^4.12.23",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "happy-dom": "^15.11.7",
    "postcss": "^8.4.49",
    "prettier": "3.1.1",
    "react-icons": "^5.4.0",
    "shx": "^0.4.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
